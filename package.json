{
  "name": "madafacker",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "prepare": "husky"
  },
  "dependencies": {
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/sequelize": "^10.0.1",
    "bull": "^4.12.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.11",
    "nestjs-cls": "^4.3.0",
    "passport": "^0.7.0",
    "passport-unique-token": "^3.0.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.3",
    "sequelize-transactional-decorator": "^1.0.0",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "uuid": "^9.0.1",
    "vercel": "^32.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 90,
        "statements": 90
      },
      "./src/controllers": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      },
      "./src/core": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      },
      "./src/frameworks": {
        "branches": 75,
        "functions": 75,
        "lines": 85,
        "statements": 75
      },
      "./src/listeners": {
        "branches": 95,
        "functions": 80,
        "lines": 95,
        "statements": 95
      },
      "./src/use-cases": {
        "branches": 88,
        "functions": 100,
        "lines": 99,
        "statements": 99
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/config/",
      "/src/app.module.ts",
      "/src/main.ts",
      "/src/frameworks/data-services/sequelize/migrations/",
      "/src/frameworks/data-services/sequelize/models/",
      "/src/frameworks/data-services/sequelize/seeders/",
      "/src/frameworks/data-services/sequelize/config"
    ],
    "moduleNameMapper": {
      "@config": [
        "<rootDir>/config"
      ],
      "@controllers": [
        "<rootDir>/controllers"
      ],
      "@core": [
        "<rootDir>/core"
      ],
      "@frameworks/(.*)": [
        "<rootDir>/frameworks/$1"
      ],
      "@jobs": [
        "<rootDir>/jobs"
      ],
      "@listeners": [
        "<rootDir>/listeners"
      ],
      "@services": [
        "<rootDir>/services"
      ],
      "@use-cases/(.*)": [
        "<rootDir>/use-cases/$1"
      ],
      "@utils/(.*)": [
        "<rootDir>/utils/$1"
      ]
    }
  }
}
